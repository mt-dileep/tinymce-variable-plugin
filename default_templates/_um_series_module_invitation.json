{
    "id": "_um_series_module_invitation",
    "name": "Module_Series_Invitaion",
    "subject": "Bulk Module and Series Invitation",
    "body": "<table style=\"font-family: open sans, helvetica, arial, verdana; border: 1px solid #eee; background: #eee;\" border=\"0\"\r\n   width=\"100%\" cellspacing=\"0\" cellpadding=\"0\">\r\n   <tbody>\r\n      <tr>\r\n         <td> </td>\r\n      </tr>\r\n      <tr>\r\n         <td> </td>\r\n      </tr>\r\n      <tr>\r\n         <td>\r\n            <center>\r\n               <table style=\"border-bottom: 1px solid #e4e4e4; border: 15px solid #fff; background: #fff;\" width=\"600\">\r\n                  <tbody>\r\n                     <tr>\r\n                        <td style=\"padding: 0; margin: 0;\" colspan=\"2\" align=\"left\">\r\n                           <a style=\"font-weight:bold;color:#000000;border:0;text-decoration:none;cursor:auto\">\r\n                           <img style=\"max-width:250px;max-height:63px;border:0;line-height:20px;display:block\" title=\"$company_name$\" src=\"$company_logo$\" alt=\"$company_name$\" />\r\n                           </a>\r\n                        </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"line-height: 20px; padding: 0; margin: 0;\" colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"border-top: 1px solid #f0f0f0; padding: 0; margin: 0;\" colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"color: #000000; font-size: 18px; padding: 0; margin: 0; font-family: open sans, helvetica, arial, verdana;\"\r\n                           colspan=\"2\">Hi $name$,\r\n                        </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"color: #000; font-size: 13px; padding: 0; margin: 0; font-family: open sans, helvetica, arial, verdana;\"\r\n                           colspan=\"2\">You have been invited to the following\r\n                           <span>\r\n                            <% for(i=0 ; i < extra.content.length && i < 2; i++) { %>\r\n                                <span><%= extra.content[i].name %></span>\r\n                                <span>(<%= extra.content[i].count %>)</span>\r\n                                <span><% if(extra.content.length > 1 && i == 0) { %></span><span> and </span><span><% } %></span>\r\n                            <% } %>\r\n                           </span><span>.</span>\r\n                           <a style=\"text-decoration: none\"\r\n                           href=\"<%= extra.dashboardUrl %>\"> Go to Dashboard</a> to view all Series and Modules assigned to you.\r\n                        </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td colspan=\"2\"><% for(i=0; i < extra.entities.length && i < 5; i++) { %></td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td colspan=\"2\" align=\"left\" valign=\"top\">\r\n                           <table style=\"width: 100%;\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n                              <tbody>\r\n                                 <tr>\r\n                                    <td valign=\"top\" width=\"164\"><a\r\n                                       style=\"font-size: 16px; font-weight: bold; color: #000000; border: 0; text-decoration: none; cursor: auto;\"\r\n                                       href=\"<%= extra.entities[i].entityUrl %>\"> <img\r\n                                       style=\"max-width: 140px; max-height: 95px; width: 140px; height: 95px; border: 0;\"\r\n                                       title=\"<%= extra.entities[i].entityName %>\"\r\n                                       src=\"<%= extra.entities[i].entityLogo %>\"\r\n                                       alt=\"<%= extra.entities[i].entityName %>\"\r\n                                       width=\"160\" height=\"95\"/> </a></td>\r\n                                    <td valign=\"top\">\r\n                                    <span><% if(extra.entities[i].entityType == \"Modules\") { %>\r\n                                        <span\r\n                                            style=\"color: #ffffff; font-size: 12px; background: <%= extra.entities[i].entityTypeBackgroundColor %>; border-style: solid; border-width: 2px 5px 2px 5px; border-color: <%= extra.entities[i].entityTypeBackgroundColor %>; font-family: open sans, helvetica, arial, verdana; display: inline-block; text-align: center;\"> \r\n                                            <span\r\n                                            style=\"visibility: hidden; font-size: 1px; line-height: 9px; width: 1px; height: 10px; color: <%= extra.entities[i].entityTypeBackgroundColor %>;\"> </span> <%= extra.entities[i].entityTypeLetter %> <span\r\n                                            style=\"visibility: hidden; font-size: 1px; line-height: 9px; color: <%= extra.entities[i].entityTypeBackgroundColor %>; width: 1px; height: 10px;\"> </span> \r\n                                        </span>\r\n                                 <% } %></span>\r\n                                    <span><% if(extra.entities[i].entityType == \"Series\") { %>\r\n                                            <img src=\"https://s3-ap-southeast-1.amazonaws.com/mtapps-cdn.mindtickle.com/mails/images/program.png\" style=\"vertical-align:middle;\" />\r\n                                        <% } %></span>\r\n                                    <span> </span>\r\n                                    <span> </span>\r\n                                          <a style=\"text-decoration: none;\" href=\"<%= extra.entities[i].entityUrl %>\">\r\n                                             <span\r\n                                                style=\"color: #0072bc; font-size: 18px; font-family: open sans, helvetica, arial, verdana;\">\r\n                                                <%= extra.entities[i].entityName %></span> </a>\r\n                                      <br/>\r\n                                      <br/>\r\n                                       <span\r\n                                          style=\"margin-left: 5px; color: #000; font-size: 13px; font-family: open sans, helvetica, arial, verdana;\"><%= extra.entities[i].entityTypeSeriesDesc %>\r\n                                       </span>\r\n                                       <span\r\n                                          style=\"position: relative; color: #000; font-size: 13px; top: 5px; font-family: open sans, helvetica, arial, verdana; line-height: 22px; \"><%= extra.entities[i].entityDescTrunc %></span>\r\n                                        <br />\r\n                                          <span\r\n                                          style=\"margin-left: 5px; position: relative; color: #999; font-size: 13px; top: 5px; font-family: open sans, helvetica, arial, verdana; line-height: 22px; \"><%= extra.entities[i].dueDate %></span>\r\n                                    </td>\r\n                                 </tr>\r\n                                 <tr>\r\n                                 <td colspan=\"2\"> </td>\r\n                                 </tr>\r\n                              </tbody>\r\n                           </table>\r\n                           <% } %>\r\n                        </td>\r\n                     </tr>\r\n                     <tr>\r\n                     <td style=\"line-height: 20px; padding: 0; margin: 0;\" colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                     <td style=\"padding-left: 20px; font-family: open sans, helvetica, arial, verdana;\" colspan=\"2\"\r\n                        align=\"center\" valign=\"top\" width=\"200\"><a\r\n                        style=\"border-radius: 5px; text-decoration: none; font-size: 14px; font-family: open sans, helvetica, arial, verdana; border: 10px solid #0072bc; background: #0072bc; display: block; width: 180px; color: white; height: 20px; line-height: 20px;\"\r\n                        href=\"<%= extra.dashboardUrl %>\" target=\"_blank\">Go to Dashboard</a></td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"padding: 0; margin: 0;\" colspan=\"2\"> </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td style=\"font-size: 13px; color: #000; font-family: open sans, helvetica, arial, verdana; line-height: 22px;\"\r\n                           colspan=\"2\" align=\"left\" valign=\"top\">Thanks,<br>$from_name$<br>$company_name$\r\n                        </td>\r\n                     </tr>\r\n             \r\n         <tr>\r\n                    <td colspan=\"2\"> </td>\r\n                     <tr style=\"text-align: center;\">\r\n                        <td colspan=\"2\" style=\"background:#fffbce;border:10px solid #fffbce;text-align:center;font-size:13px;font-family:open sans, helvetica, arial, verdana;\">\r\n                           Learning Site URL: <a style=\"color:#0072bc;font-size:12px;text-decoration:none\" href=\"<%= extra.dashboardUrl %>\" target=\"_blank\"><%= extra.dashboardUrlText %></a>\r\n                        </td>\r\n                     </tr>\r\n                     <tr>\r\n                        <td> </td>\r\n                     </tr>\r\n                  </tbody>\r\n               </table>\r\n            </center>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\">\r\n            <center>\r\n               <table style=\"border-top: 15px solid #f9f9f9; border-bottom: 15px solid #f9f9f9; border-left: 10px solid #f9f9f9; border-right: 10px solid #f9f9f9; background: #f9f9f9;\"\r\n                  width=\"600\" cellspacing=\"0\" cellpadding=\"0\">\r\n                  <tbody>\r\n                     <tr>\r\n                        <td style=\"line-height: 18px; padding: 0; margin: 0; font-family: open sans, helvetica, arial, verdana;\">\r\n                           <span style=\"color: #7c7c7c; font-size: 12px; font-family: open sans, helvetica, arial, verdana;\">This email was sent to </span>\r\n                           <a style=\"color: #0072bc; font-size: 12px; text-decoration: none; font-family: open sans, helvetica, arial, verdana;\"\r\n                              href=\"mailto:$email$\" target=\"_blank\">$email$.</a> <span\r\n                              style=\"color: #7c7c7c; font-size: 12px; font-family: open sans, helvetica, arial, verdana;\">You received this email because you were invited to</span>\r\n                           <a style=\"color: #0072bc; font-size: 12px; text-decoration: none; font-family: open sans, helvetica, arial, verdana;\"\r\n                              href=\"<%= extra.dashboardUrl %>\" target=\"_blank\"><%= extra.dashboardUrlText %>.</a>\r\n                        </td>\r\n                     </tr>\r\n                  </tbody>\r\n               </table>\r\n            </center>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\">\r\n            <center>\r\n               <table style=\"border: 0px;\" width=\"600\">\r\n                  <tbody>\r\n                     <tr>\r\n                        <td style=\"border: 10px solid #eee; background: #eee;\" align=\"right\" valign=\"top\"><img\r\n                           style=\"display: block;\"\r\n                           src=\"https://s3-ap-southeast-1.amazonaws.com/mtapps-cdn.mindtickle.com/mails/images/mt-logo.png\"\r\n                           alt=\"\"/></td>\r\n                     </tr>\r\n                  </tbody>\r\n               </table>\r\n            </center>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\"> </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\"> </td>\r\n      </tr>\r\n   </tbody>\r\n</table>",
    "from": "$from_name$",
    "cc": [],
    "company": "_default",
    "owner": "_default",
    "lastUpdatedBy": "_default",
