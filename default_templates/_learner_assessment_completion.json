    "subject": "Assessment Result | $course_name$",
    "body": "<table width=\"100%\"  cellspacing=\"0\" cellpadding=\"0\" rowspan=\"0\" colspan=\"0\" border=\"0\"  style=\"font-family: open sans, helvetica, arial, verdana; border:1px solid #eee; background:#eee;\">\r\n    \r\n    <tr><td>&nbsp;</td></tr>\r\n    \r\n    <tr><td>&nbsp;</td></tr>\r\n    \r\n    <tr>\r\n\t\t    <td>\r\n\t\t\t    <center>\r\n                    \r\n\t\t\t\t    <table width=\"600\" style=\"border-bottom:1px solid #e4e4e4;border:15px solid #fff;background:#fff;\">\r\n\t\t\t\t\t    <tr>\r\n\t\t\t\t\t\t    <td  colspan=\"2\" align=\"left\" style=\"padding:0;margin:0\">\r\n\t\t\t\t\t\t\t\t<a style=\"font-family: Open sans;font-size:16px;font-weight:bold;color:#000000;border:0;text-decoration:none;cursor:auto\">\r\n\t\t\t\t\t\t\t\t    <img style=\"max-width:250px;max-height:63px;border:0;line-height:20px;display:block\" title=\"$company_name$\" src=\"$company_logo$\" alt=\"$company_name$\">\r\n\t\t\t\t\t\t\t\t</a>\r\n                                \r\n\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t<td  colspan=\"2\" style=\"line-height:20px;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t<td  colspan=\"2\" style=\"border-top:1px solid #f0f0f0;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"color:#000000;font-size:18px;padding:0;margin:0;font-family: open sans, helvetica, arial, verdana;\">Hi $name$,</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"line-height:15px;padding:0;margin:0\">&nbsp;</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"color:#999;font-family: open sans, helvetica, arial, verdana;font-size:13px;padding:0;margin:0\"><%= extra.completionMailStmt %></td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" style=\"line-height:22px;padding:0;margin:0\">&nbsp;</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td align=\"left\" valign=\"top\" width=\"200\" style=\"padding-right:20px;\">\r\n                                <img width=\"200\" height=\"135\" style=\"max-width:200px;max-height:135px;color:#000000;font-size:16px;display:block\" src=\"$course_thumb$\" title=\"$course_name$\" alt=\"$course_name$\">\r\n                            </td>\r\n                            <td align=\"center\" valign=\"middle\" style=\"color:#000;font-size:13px\">\r\n                                <div style=\"font-size:24px;text-align:center;color:<%= extra.scoreTxtColor %>\">         <span style=\"font-weight:bold;font-family: open sans, helvetica, arial, verdana;\"><%= extra.asmtUserScore %></span> /<%=extra.asmtMaxScore %> pts\r\n                                </div>    \r\n                                <div style=\"color:#999;font-size:13px;text-decoration:none;text-                        align:center;margin-bottom:8px;display:block; font-family: Helvetica, Arial, sans-serif;\">(Passing score <%=extra.asmtPassingScore %>)\r\n                                </div>\r\n                                \r\n                                <div>\r\n                                  <a style=\"line-height:34px;font-size:14px;color:#999;border-radius:3px;border:1px solid #999;text-decoration:none;width:180px;font-family: open sans, helvetica, arial, verdana;text-align:center;display:block;text-decoration:none;cursor:auto;display:$certificate_display$\" href=\"$certificate_url$\">Download Certificate</a>\r\n                                    <div>&nbsp;</div>\r\n\t\t\t\t\t\t<a style=\"line-height:34px;font-size:14px;color:#999;border-radius:3px;width:280px;border:1px solid #999;text-decoration:none;text-align:center;display:block;text-decoration:none;cursor:auto;display:<%= extra.reattemptBtnDisplay %>\" href=\"$link$\"><%= extra.reAttemptBtnTxt %></a>\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                            \r\n                            \r\n                        \r\n                        \r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t<td  colspan=\"2\" style=\"line-height:20px;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t<td  colspan=\"2\" style=\"line-height:20px;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t    <td  colspan=\"2\" align=\"left\" valign=\"top\" style=\" line-height:22px;font-family: open sans, helvetica, arial, verdana;font-size:13px; text-decoration:none;\">\r\n                                Thanks,<br>$from_name$<br>$company_name$\r\n\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t<td   colspan=\"2\" style=\"line-height:40px;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n                        <tr>\r\n                            <td  colspan=\"2\" style=\"background:#fffbce;border:10px solid #fffbce;text-align:center;font-size:13px;font-family: open sans, helvetica, arial, verdana;\">\r\n\t\t\t\t\t\t\t\t<span style=\"color:#000000;font-family: open sans, helvetica, arial, verdana;\">Learning Site URL:</span>\r\n\t\t\t\t\t\t\t\t<a style=\"color:#0072bc;font-size:12px;text-decoration:none;font-family: open sans, helvetica, arial, verdana;\" href=\"$company_link$\" target=\"_blank\">$company_link_text$.</a>\r\n\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t</tr>\r\n                                    \r\n                               \r\n                        <tr>\r\n\t\t\t\t\t\t\t<td  colspan=\"2\" style=\"line-height:10px;padding:0;margin:0\">&nbsp;</td>\r\n\t\t\t\t\t\t</tr>\r\n                        <tr>\r\n                            <td colspan=\"2\" align=\"center\" style=\"font-style:italic;font-size:12px;color:#999;font-family: open sans, helvetica, arial, verdana;\">Note: Few Mission types are not yet available on the mobile app.\r\n                            </td>\r\n                            \r\n                         </tr>\r\n                                \r\n\t\t\t\t\t</table>\r\n\r\n\t\t\t\t</center>\r\n\t\t\t</td>\r\n\t\t</tr>\r\n    \r\n\r\n    <tr>\r\n        <td>\r\n\r\n                <center>\r\n                <table width=\"600\" cellspacing=\"0\" cellpadding=\"0\" style=\"border-top:15px solid #f9f9f9;border-bottom:15px solid #f9f9f9;border-left:10px solid #f9f9f9;border-right:10px solid #f9f9f9;background:#f9f9f9\">\r\n\r\n                    <tr>\r\n                        <td style=\"line-height:18px;padding:0;margin:0;\">\r\n                            <span style=\"color:#7c7c7c;font-size:12px;font-family: open sans, helvetica, arial, verdana;\">This email was sent to </span>\r\n                            <a style=\"color:#0072bc;font-size:12px;text-decoration:none;font-family: open sans, helvetica, arial, verdana;\" href=\"mailto:$email$\" target=\"_blank\">$email$.</a>\r\n                            <span style=\"color:#7c7c7c;font-size:12px;font-family: open sans, helvetica, arial, verdana;\">You received this email because you were invited to</span>\r\n                            <a style=\"color:#0072bc;font-size:12px;text-decoration:none;font-family: open sans, helvetica, arial, verdana;\" href=\"$company_link$\" target=\"_blank\">$company_name$.</a>\r\n\r\n                        </td>\r\n                    </tr>\r\n                </table>\r\n\r\n            </center>\r\n            </td>\r\n        </tr>\r\n\r\n\r\n    <tr>\r\n            <td>\r\n\r\n                <center>\r\n                    <table width=\"600\" style=\"border:0px\">\r\n                        <tr>\r\n                            <td align=\"right\" valign=\"top\" style=\"border:10px solid #eee;background: #eee;\">\r\n                                <img src=\"https://s3-ap-southeast-1.amazonaws.com/mtapps-cdn.mindtickle.com/mails/images/mt-logo.png\" style=\"display:block\"/>\r\n                            </td>\r\n                        </tr>\r\n                    </table>\r\n                </center>\r\n            </td>\r\n        </tr>\r\n    <tr><td>&nbsp;</td></tr>\r\n    <tr><td>&nbsp;</td></tr>\r\n</table>",
